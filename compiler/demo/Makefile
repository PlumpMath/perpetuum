all:
	gcc -o development_net.o -c -I /usr/local/src/perpetuum/include/ development_net.c
	gcc -DPETRINET_SINGLETONS -o development_net.o -c -I /usr/local/src/perpetuum/include/ development_net.c
	gcc -DPETRINET_SINGLETONS -DPETRINET_GLOBALS -o development_net.o -c -I /usr/local/src/perpetuum/include/ development_net.c
